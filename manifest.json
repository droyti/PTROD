{
    "name": "PTROD",
    "version": "1.0",
    "description": "Protest the Removal of Dislikes by automatically disliking every YouTube video you watch!",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["dislike.js"]
        }
      ],   
    "browser_action": {
      "default_popup": "gui/about.html"
    },
    "manifest_version": 2
}